{
  "name": "umap-js",
  "version": "1.4.0",
  "description": "JavaScript implementation of UMAP",
  "author": {
    "name": "Andy Coenen",
    "email": "andycoenen@google.com"
  },
  "license": "MIT",
  "repository": "PAIR-code/umap-js",
  "main": "dist/index.js",
  "unpkg": "lib/umap-js.min.js",
  "jsdelivr": "lib/umap-js.min.js",
  "types": "dist/index.d.ts",
  "packageManager": "yarn@4.12.0",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "bundle": "rm -rf lib && webpack --config ./webpack/lib.config.ts && webpack --config ./webpack/lib.min.config.ts",
    "build": "rm -rf dist && tsc && yarn bundle",
    "build:wasm": "cd wasm && rm -rf pkg && wasm-pack build --target web --out-dir pkg/web && wasm-pack build --target nodejs --out-dir pkg/node"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@vitest/ui": "^2.1.8",
    "prando": "^5.1.2",
    "prettier": "^3.7.4",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitest": "^2.1.8",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "ml-levenberg-marquardt": "^2.1.1"
  },
  "publishConfig": {
    "registry": "https://wombat-dressing-room.appspot.com"
  }
}
